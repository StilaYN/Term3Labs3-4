<Window x:Class="PhotoEditor.EnterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotoEditor" xmlns:modelview="clr-namespace:PhotoEditor.ModelView" d:DataContext="{d:DesignInstance Type=modelview:PhotoEditorMainModelView}"
        mc:Ignorable="d"
        Title="ProjectSetting" Height="150" Width="250">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="31,25,0,0" TextWrapping="Wrap" Text="{Binding Width}" VerticalAlignment="Top" Width="69" Height="17"/>
        <TextBox HorizontalAlignment="Left" Margin="31,67,0,0" TextWrapping="Wrap" Text="{Binding Height}" VerticalAlignment="Top" Width="69"/>
        <Button Content="Создать" Command="{Binding Create}" HorizontalAlignment="Left" Margin="126,25,0,0" VerticalAlignment="Top" Width="60"/>
        <TextBlock HorizontalAlignment="Left" Margin="31,10,0,0" TextWrapping="Wrap" Text="Ширина" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="31,51,0,0" TextWrapping="Wrap" Text="Высота" VerticalAlignment="Top"/>

    </Grid>
</Window>
